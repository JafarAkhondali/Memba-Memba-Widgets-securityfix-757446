{
    "env": {
        "test": {
            "plugins": ["istanbul"]
        }
    },
    // Webpack does not need the plugin section to build, kept for SystemJS
    "plugins": [
        [
            "module-resolver",
            {
                "cwd": "babelrc", // "packagejson"
                "extensions": [".es6", ".js", ".mjs"],
                "root": [
                    ".",
                    "./test/vendor/",
                    // "./src/js/vendor/**",
                    "./src/js/vendor/kendo/",
                    "./src/js/vendor/kendo/cultures/",
                    "./src/js/vendor/kendo/messages/"
                ],
                "alias": {
                    // This takes over externals in webpack and includes jQuery in the builds
                    // "jquery": "./src/js/vendor/jquery/jquery-3.6.0.min.js"
                }
            }
        ]
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "corejs": 3,
                "targets": {
                    "chrome": "100",
                    "ie": "11"
                },
                "useBuiltIns": "entry"
            }
        ]
    ]
}
