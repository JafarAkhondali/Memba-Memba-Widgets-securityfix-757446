{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "prettier"
    ],
    "rules": {
        "no-underscore-dangle": 0,
        "prettier/prettier": ["error", {
            "singleQuote": true,
            "tabWidth": 4
        }],
        "strict": 0
    },
    "settings": {
        "import/resolver": {
            "babel-module": {}
        }
    }
}
