  "scripts": {
    "docs": "jsdoc -c ./jsdoc.json",
    "eslint": "eslint . ./src/js/**/*.{es6,jsx} ./templates/**/*.es6 ./test/**/*.es6",
    "karma": "karma start ./coverage.conf.js",
    "modernizr": "customizr -c ./src/js/vendor/modernizr/modernizr.json && customizr -c ./src/js/vendor/modernizr/modernizr.min.json",
    "prosemirror": "rollup -c ./src/js/vendor/prosemirror/rollup.config.js",
    "sloc": "sloc -a es6=js,ejs=html -d -e \".idea|node_modules|vendor\" -f cli-table .",
    "start": "node ./nodejs/http.server.js",
    "stylelint": "stylelint ./src/styles/**/*.scss",
    "test": "npm run karma",
    "upgrade": "ncu -u -x chai && npm i"
  },
